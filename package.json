{
  "name": "syncthing-desktop",
  "productName": "Syncthing Desktop",
  "version": "0.3.0",
  "description": "A Syncthing GUI for macOs",
  "license": "MIT",
  "repository": "JodusNodus/syncthing-desktop",
  "main": "./dist/main/index.js",
  "author": {
    "name": "jodusnodus",
    "email": "thomas842@hotmail.com",
    "url": "thomasbilliet.com"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test.setup.js --recursive app/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "lint": "eslint app test *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "babel ./app/main --out-dir ./dist/main",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack/webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "build-main:watch": "npm run build-main -- --watch",
    "start": "cross-env NODE_ENV=development HOT=1 electron ./",
    "package": "npm run build && cross-env NODE_ENV=production node -r babel-register package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "presets": [
          "react-hmre"
        ]
      },
      "test": {
        "plugins": [
          [
            "webpack-loaders",
            {
              "config": "./webpack/webpack.config.node.js",
              "verbose": false
            }
          ]
        ]
      }
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
    "auto-launch": "^2.1.0",
    "chart.js": "^2.1.6",
    "css-modules-require-hook": "^4.0.0",
    "deep-equal": "^1.0.1",
    "electron-debug": "^0.6.0",
    "electron-json-storage": "^2.0.0",
    "font-awesome": "^4.6.1",
    "is-electron-renderer": "^2.0.0",
    "lodash": "^4.13.1",
    "moment": "^2.14.1",
    "node-syncthing": "^3.3.1",
    "pixl-xml": "^1.0.7",
    "postcss": "^5.0.19",
    "qrcode.react": "^0.6.1",
    "randomstring": "^1.1.5",
    "react": "^0.14.1",
    "react-desktop": "^0.2.3",
    "react-dom": "^0.14.1",
    "react-hyperscript": "^2.4.0",
    "react-photonkit": "^0.4.1",
    "react-progressbar": "^1.2.0",
    "react-qr-reader": "^0.2.7",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.4",
    "react-router-redux": "^4.0.2",
    "react-simple-dropdown": "^1.1.4",
    "redux": "^3.5.2",
    "redux-electron-store": "^0.3.10",
    "redux-form": "^5.3.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.3",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^7.0.0",
    "electron-prebuilt": "^1.2.6",
    "electron-rebuild": "^1.1.3",
    "enzyme": "^2.4.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.6.0",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "sass-loader": "^4.0.0",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devEngines": {
    "node": "4.x || 5.x || 6.x",
    "npm": "2.x || 3.x"
  }
}
